{
  "name": "floor12/yii2-module-mailing",
  "description": "Mailing module for yii2 projects.",
  "type": "yii2-module",
  "minimum-stability": "dev",
  "keywords": [
    "floor12",
    "yii2 module",
    "mailing",
    "email",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "floor12@protonmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "voskobovich/yii2-linker-behavior": "@dev",
    "kartik-v/yii2-widget-select2": "@dev",
    "yii2mod/yii2-enum": "*",
    "floor12/yii2-module-files": "*",
    "floor12/yii2-editmodal": "*",
    "floor12/yii2-summernote": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\mailing\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\mailing\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-files"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-summernote"
    }
  ]
}
